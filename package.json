{
  "name": "linkinvests",
  "version": "1.0.0",
  "description": "Real estate investment platform - Monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.18.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm --filter './packages/*' build",
    "lint": "pnpm --filter './packages/*' lint",
    "lint:fix": "pnpm --filter './packages/*' lint:fix",
    "format": "pnpm --filter './packages/*' format",
    "test": "pnpm --filter './packages/*' test",
    "typecheck": "pnpm --filter './packages/*' typecheck",
    "dev": "pnpm --filter './packages/*' dev",
    "init-energy-diags": "tsx scripts/init-energy-diags.ts",
    "init-deceases": "tsx scripts/init-deceases.ts",
    "init-liquidations": "tsx scripts/init-liquidations.ts",
    "init-notary-listings": "tsx scripts/init-notary-listings.ts",
    "init-moteur-immo": "tsx scripts/init-moteur-immo.ts",
    "setup-test-db": "tsx scripts/setup-test-db.ts"
  },
  "keywords": [
    "real-estate",
    "investment",
    "monorepo"
  ],
  "author": "",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "zod": "^4.1.12",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1"
  }
}
